{
  "name": "read-json-safe",
  "version": "1.0.1",
  "description": "Read objects from JSON files without try catch.",
  "author": "Connor White",
  "license": "MIT",
  "keywords": [
    "json",
    "read",
    "file",
    "safe",
    "try",
    "catch",
    "ENOENT"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bconnorwhite/read-json-safe.git"
